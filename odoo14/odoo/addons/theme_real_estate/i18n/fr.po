#: model_terms:theme.ir.ui.view,arch:theme_real_estate.s_quotes_carousel
msgid ""
"<span class=\"s_blockquote_author\"><b>Jane Doe</b> • Customer, bought a "
"house</span>"
msgstr ""
"<span class=\"s_blockquote_author\"><b>Jane Doe</b> • Cliente, a acheté une "
"maison</span>"

#. module: theme_real_estate
#: model_terms:theme.ir.ui.view,arch:theme_real_estate.s_quotes_carousel
